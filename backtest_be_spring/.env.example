# Copy to .env for local development. Override values as needed.
SPRING_DATASOURCE_URL=jdbc:mysql://localhost:3306/stock_community?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC
SPRING_DATASOURCE_USERNAME=bt_user
SPRING_DATASOURCE_PASSWORD=bt_pass
SPRING_DATASOURCE_POOL_SIZE=10

# OAuth sample placeholders
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# JWT secret must be at least 64 characters when running locally.
APP_SECURITY_JWT_SECRET=change-me-default-secret-key-that-is-at-least-64-characters-long-1234567890
